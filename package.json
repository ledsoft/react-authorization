{
  "name": "react-authorization",
  "version": "0.0.1",
  "description": "React authorization library.",
  "keywords": [
    "react",
    "authorization"
  ],
  "author": {
    "name": "Martin Ledvinka",
    "email": "martin.ledvinka@hotmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ledsoft/react-authorization.git"
  },
  "dependencies": {
    "babel-runtime": "^6.9.2"
  },
  "peerDependencies": {
    "react": ">= 15.0.1",
    "react-dom": ">= 15.0.1"
  },
  "main": "lib/react-authorization.js",
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.5",
    "babel-eslint": "^6.0.2",
    "babel-jest": "^11.0.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-runtime": "^6.7.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "react": "^15.4.1",
    "react-addons-test-utils": "^15.4.1",
    "react-dom": "^15.4.1",
    "watchify": "^3.7.0",
    "webpack": "^1.12.15",
    "jsdom": "^8.3.1",
    "jasmine": "^2.4.1",
    "jasmine-core": "^2.4.1",
    "jasmine-reporters": "^2.1.1"
  },
  "scripts": {
    "test": "jasmine JASMINE_CONFIG_PATH=test/jasmine.json",
    "build": "browserify ./src/react-authorization.js -o ./dist/react-authorization.js",
    "watchify": "watchify ./src/react-authorization.js -o ./dist/react-authorization.js",
    "lib": "babel ./src -d ./lib",
    "build-npm-prod": "npm run test && npm run build && npm run lib && npm pack",
    "prebuild-npm-prod": "npm install"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  }
}
